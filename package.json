{
    "main": "./src/starter.js",
    "name": "super_stats",
    "version": "1.0.5",
    "scripts": {
        "start": "electron ./src/starter.js",
        "dist": "electron-builder ",
        "compile": "electron ./src/compiler.src.js",
        "recompile": " rm -rf ./src/*.jsc && sleep 1 && electron ./src/compiler.src.js && ^C"
    },
    "build": {
        "files": [
            {
                "filter": [
                    "!./src/main.src.js",
                    "!./src/compiler.src.js",
                    "!./src/contacts.db",
                    "./src/public",
                    "./src/index.html",
                    "./src/starter.js",
                    "!./src/ic.ico",
                    "./src/main.jsc",
                    "./src/compiler.jsc",
                    "./src/client.js",
                    "./src/test.db"
                ]
            }
        ],
        "extends": null,
        "appId": "Super-Stats",
        "win": {
            "target": [
                "nsis"
            ],
            "icon": "./src/ic.ico"
        },
        "nsis": {
            "installerIcon": "./src/ic.ico",
            "uninstallerIcon": "./src/ic.ico",
            "uninstallDisplayName": "Super Stats",
            "oneClick": false,
            "allowToChangeInstallationDirectory": true
        }
    },
    "dependencies": {
        "@journeyapps/sqlcipher": "^5.2.0",
        "bytenode": "^1.3.2",
        "electron-forge": "^5.2.4"
    },
    "devDependencies": {
        "electron-builder": "^22.11.3",
          "electron": "^12.0.7"
    }
}
